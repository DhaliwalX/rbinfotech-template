        // Bootstrap icons
        link(href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css', rel='stylesheet', type='text/css')

        // Google fonts
        link(href='https://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic,700italic', rel='stylesheet', type='text/css')

        // Core theme CSS (includes Bootstrap)
        link(href='css/styles.css', rel='stylesheet')

    body(style='background-color:#eaeaea')

        // Navigation
        nav.navbar.navbar-light.bg-light.static-top
            .container.position-relative
                .row.align-items-center
                    .col-lg-2
                        .container
                            .row.align-items-center
                                .col
                                    img.img-fluid(src='assets/img/apple-icon.png', style='width:40px; height:auto') 
                                .col
                                    a.navbar-brand(href='#!') RB Infotech
                    .col-lg-2
                //a.btn.btn-primary(href='#signup') Sign Up

        // Masthead
        header.masthead
            .container.position-relative
                .row.justify-content-center
                    .col-xl-6
                        .text-center.text-white
                            // Page heading
                            h1.mb-3 Simply Innovate
                            p.center We at RB Infotech, strive to provide innovative solutions to the digital market worldwide.

                            // Signup form

